package dmw.test.statements;

@java

/**
 * This widget defines test for while statement.
 */
class G : Widget {
	
	class R : Object {
		public var number : Int;
		public var text : String;
	}
	
	public var r : R;
	
	public var array : Array <r> @max(99);
	
	
	public func getSum() -> Int {			
		sum : Int = 0;
		i : Int = 0;
		repeat {
			sum = sum + 1;
			i = i + 1;
		} while (i < 10)	
		return sum;
	}
		
}				
